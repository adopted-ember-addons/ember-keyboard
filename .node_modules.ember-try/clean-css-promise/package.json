{
  "name": "clean-css-promise",
  "version": "0.1.1",
  "description": "Promises/A+ version of clean-css",
  "repository": "shinnn/clean-css-promise",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "pretest": "eslint --config @shinnn/node src.js test.js && babel --presets es2015 src.js --out-file index.js",
    "test": "node --strong_mode --throw-deprecation --track-heap-objects test.js",
    "coverage": "node --strong_mode node_modules/.bin/istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "keywords": [
    "css",
    "clean-css",
    "cleancss",
    "style",
    "styles",
    "stylesheet",
    "minifier",
    "minification",
    "compress",
    "optimization",
    "optimize",
    "optimizer",
    "promise",
    "promises",
    "promisified",
    "then",
    "thenable"
  ],
  "dependencies": {
    "array-to-error": "^1.0.0",
    "clean-css": "^3.4.5",
    "pinkie-promise": "^2.0.0"
  },
  "devDependencies": {
    "@shinnn/eslint-config-node": "^1.0.1",
    "babel-cli": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "eslint": "^1.6.0",
    "istanbul": "^0.3.22",
    "istanbul-coveralls": "^1.0.3",
    "tape": "^4.2.1"
  }
}
