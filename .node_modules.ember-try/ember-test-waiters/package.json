{
  "name": "ember-test-waiters",
  "version": "1.2.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "docs": "ember ts:precompile && documentation build --document-exported \"addon/index.js\" --document-exported \"addon-test-support/index.js\" --config documentation.yml --markdown-toc-max-depth 3 -f md -o API.md && prettier --single-quote --trailing-comma es5 --write API.md && ember ts:clean",
    "lint:js": "eslint --ext ts,js .",
    "lint:ts": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "test:node": "qunit node-tests",
    "test:prod": "ember test --environment=production"
  },
  "dependencies": {
    "ember-cli-babel": "^7.11.0",
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "@glimmer/env": "^0.1.7",
    "@types/ember": "^3.1.1",
    "@types/ember-test-helpers": "^1.0.6",
    "@types/ember-testing-helpers": "^0.0.3",
    "@types/ember__test-helpers": "^0.7.7",
    "@types/qunit": "^2.9.0",
    "@types/rsvp": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "documentation": "^9.3.1",
    "ember-cli": "~3.8.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-typescript": "^1.5.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.7.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.2.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "loader.js": "^4.7.0",
    "prettier": "^1.18.2",
    "qunit": "^2.9.2",
    "release-it": "^12.2.1",
    "release-it-lerna-changelog": "^1.0.3",
    "typescript": "^3.5.3"
  },
  "resolutions": {
    "**/engine.io": "~3.3.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}",
      "changelog": "git log --pretty=format:\"* %s (%h)\" --no-merges --perl-regexp --author=\"^((?!dependabot-preview).*)$\" ${latestTag}...HEAD"
    },
    "github": {
      "release": true
    }
  }
}
