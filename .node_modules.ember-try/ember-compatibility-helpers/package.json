{
  "name": "ember-compatibility-helpers",
  "version": "1.2.2",
  "description": "Zero-cost compatibility flags and helpers for Ember.js",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/pzuraq/ember-compatibility-helpers",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests node-tests",
    "prepack": "node ./build/write-declaration-file",
    "start": "ember serve",
    "test": "mocha node-tests/babel-7",
    "test:all": "ember try:each"
  },
  "resolutions": {
    "testem": "~2.0.0"
  },
  "dependencies": {
    "babel-plugin-debug-macros": "^0.2.0",
    "ember-cli-version-checker": "^5.1.1",
    "semver": "^5.4.1"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "broccoli-test-helper": "^1.1.0",
    "chai": "^4.1.0",
    "co": "^4.6.0",
    "console-ui": "^1.0.3",
    "core-object": "^3.1.3",
    "ember-cli": "~3.0.0",
    "ember-cli-babel": "^7.1.1",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-data": "^3.5.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.0.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^5.2.1",
    "loader.js": "^4.2.3",
    "mocha": "^3.4.2",
    "release-it": "^14.2.1",
    "release-it-lerna-changelog": "^3.1.0"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
