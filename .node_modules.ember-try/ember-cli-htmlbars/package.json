{
  "name": "ember-cli-htmlbars",
  "version": "5.3.1",
  "description": "A library for adding htmlbars to ember CLI",
  "keywords": [
    "ember-addon",
    "ember-cli"
  ],
  "homepage": "https://github.com/ember-cli/ember-cli-htmlbars",
  "bugs": {
    "url": "https://github.com/ember-cli/ember-cli-htmlbars/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ember-cli/ember-cli-htmlbars.git"
  },
  "license": "MIT",
  "author": "Jonathan Jackson & Chase McCarthy",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --cache .",
    "start": "ember serve",
    "test": "yarn run test:ember && yarn run test:node",
    "test:ember": "ember test",
    "test:node": "mocha node-tests/*.js",
    "test:node:debug": "mocha debug node-tests/*.js"
  },
  "dependencies": {
    "@ember/edition-utils": "^1.2.0",
    "babel-plugin-htmlbars-inline-precompile": "^4.2.0",
    "broccoli-debug": "^0.6.5",
    "broccoli-persistent-filter": "^3.1.0",
    "broccoli-plugin": "^4.0.3",
    "common-tags": "^1.8.0",
    "ember-cli-babel-plugin-helpers": "^1.1.0",
    "fs-tree-diff": "^2.0.1",
    "hash-for-dep": "^1.5.1",
    "heimdalljs-logger": "^0.1.10",
    "json-stable-stringify": "^1.0.1",
    "semver": "^7.3.2",
    "silent-error": "^1.1.1",
    "strip-bom": "^4.0.0",
    "walk-sync": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/plugin-transform-typescript": "^7.10.1",
    "@babel/runtime": "^7.10.2",
    "@ember/optional-features": "^1.3.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-debug-macros": "^0.3.3",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-test-helper": "^2.0.0",
    "chai": "^4.2.0",
    "co": "^4.6.0",
    "console-ui": "^3.1.1",
    "core-object": "^3.1.5",
    "ember-cli": "~3.18.0",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-version-checker": "^5.1.1",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.19.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.9.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ember": "^8.9.2",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fixturify": "^2.1.0",
    "loader.js": "^4.7.0",
    "mocha": "^8.1.0",
    "module-name-inliner": "link:./tests/dummy/lib/module-name-inliner",
    "prettier": "^2.0.5",
    "qunit-dom": "^1.3.0",
    "release-it": "^13.6.4",
    "release-it-lerna-changelog": "^2.3.0"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "main": "lib/ember-addon-main.js",
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
